{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "NODE_ENV=production npm run prod",
    {{#if_eq runner "jest"}}
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    {{/if_eq}}
    {{#if_eq runner "karma"}}
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    {{/if_eq}}
    {{#e2e}}
    "e2e": "node test/e2e/runner.js",
    {{/e2e}}
    {{#if_or unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/if_or}}
    {{#lint}}
    "lint": "eslint --ext .js,.vue src{{#unit}} test/unit{{/unit}}{{#e2e}} test/e2e/specs{{/e2e}}",
    {{/lint}}
    "build": "node build/build.js",
    "prod": "node server.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "fastify": "^1.6.0",
    "fastify-compress": "^0.6.0",
    "fastify-static": "^0.12.0",
    "vue": "^2.5.16"{{#router}},
    "vue-router": "^3.0.1"{{/router}}
  },
  "devDependencies": {
    {{#lint}}
    "babel-eslint": "^8.2.1",
    "eslint": "^4.19.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.5.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    {{/if_eq}}
    {{/lint}}
    {{#if_eq runner "jest"}}
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.51",
    "jest": "^23.1.0",
    "jest-serializer-vue": "^2.0.2",
    "vue-jest": "^2.6.0",
    {{/if_eq}}
    {{#if_eq runner "karma"}}
    "cross-env": "^5.0.1",
    "karma": "^2.0.2",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.1",
    "chai": "^4.1.2",
    "sinon": "^4.0.0",
    "sinon-chai": "^3.0.0",
    "inject-loader": "^4.0.1",
    "babel-plugin-istanbul": "^4.1.6",
    "phantomjs-prebuilt": "^2.1.14",
    {{/if_eq}}
    {{#e2e}}
    "@babel/register": "^7.0.0-beta.51",
    "chromedriver": "^2.38.2",
    "geckodriver": "^1.11.0",
    "cross-spawn": "^6.0.5",
    "nightwatch": "^1.0.3",
    "selenium-server": "^3.11.0",
    {{/e2e}}
    "autoprefixer": "^8.6.2",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "node-notifier": "^5.2.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "ora": "^2.1.0",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "portfinder": "^1.0.13",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.6.0",
    "yarn": "^1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
